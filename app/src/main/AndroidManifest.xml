<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.util.customview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.util.customview.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.util.customview.custom_view.CustomViewActivity" />
        <activity android:name="com.util.customview.animation_parabola.ParabolaActivity" />
        <activity android:name="com.util.customview.auto_scroll.AutoScrollActivity" />
        <activity android:name="com.util.customview.animation_wave.WaveAnimActivity" />
    </application>

</manifest>
