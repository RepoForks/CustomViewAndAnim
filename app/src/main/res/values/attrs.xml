<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RoundImageView">
        <attr name="type"/>
        <attr name="radius"/>
        <attr name="has_frame" format="boolean"/>
        <attr name="has_mask" format="boolean"/>
    </declare-styleable>

    <attr name="type">
        <enum name="circle" value="0"/>
        <enum name="round" value="1"/>
    </attr>
    <attr name="radius" format="dimension"/>

    <declare-styleable name="MaskFrameLayout">
        <attr name="border_radius" format="dimension"/>
    </declare-styleable>

</resources>